<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="36.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="swen2.tp.swen2_tp_hw.view.ListMenuController">
    <children>
        <HBox prefHeight="45.0" prefWidth="600.0" spacing="12.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label text="Tours" underline="true">
                    <font>
                        <Font name="Arial Bold" size="14.0" />
                    </font>
                    <padding>
                        <Insets right="10.0" top="6.0" />
                    </padding>
                </Label>
                <Button fx:id="btn_createTour" mnemonicParsing="false" onAction="#onAddTourClick" prefHeight="20.0" prefWidth="35.0" style="-fx-background-color: #d3d3d3; -fx-border-color: black;" text="+" />
                <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="35.0" style="-fx-background-color: #d3d3d3; -fx-border-color: black;" text="-" />
                <Button mnemonicParsing="false" prefHeight="20.0" prefWidth="35.0" style="-fx-background-color: #d3d3d3; -fx-border-color: black;" text="[ ]" />
            </children>
            <padding>
                <Insets left="6.0" top="4.0" />
            </padding>
        </HBox>
    </children>
</AnchorPane>
